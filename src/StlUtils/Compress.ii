#if defined( STLUTILS_DEBUG )
#define inline
#endif

inline
DumpInfo< Compress >
Compress::dump( const char * prefix, bool showVer ) const
{
  return( DumpInfo< Compress >( *this, prefix, showVer ) );
}

inline
bool
Compress::compress( const FilePath & inFn, const FilePath & outFn )
{
  return( compress( inFn.c_str(), outFn.c_str() ) );
}

inline
bool
Compress::decompress( const FilePath & inFn, const FilePath & outFn )
{
  return( decompress( inFn.c_str(), outFn.c_str() ) );
}

