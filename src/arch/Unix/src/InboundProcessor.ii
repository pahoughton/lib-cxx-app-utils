template< class Proc >
inline
InboundProcessor<Proc>::InboundProcessor(
  const char *	    fileNamePattern,
  const char *	    inDirName,
  const char *	    procDirName,
  long		    rescanWaitSecs,
  Proc &	    processor
  )
  : InboundProcessorBase( fileNamePattern,
			  inDirName,
			  procDirName,
			  rescanWaitSecs ),
    proc( processor )
{
}

template< class Proc >
inline
InboundProcessor<Proc>::InboundProcessor(
  const char *		fileNamePattern,
  const char *		inDirName,
  const char *		procDirName,
  long			rescanWaitSecs,
  const SigCatcher *	sigCatcher,
  Proc &		processor
  )
  : InboundProcessorBase( fileNamePattern,
			  inDirName,
			  procDirName,
			  rescanWaitSecs,
			  sigCatcher ),
    proc( processor )
{
}

